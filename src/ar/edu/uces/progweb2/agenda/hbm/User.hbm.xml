<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="ar.edu.uces.progweb2.agenda.model.User" table="usuario">
		<id column="id" name="id">
			<generator class="native" />
		</id>
		<property column="usuario" name="user" />
		<property column="contrasenia" name="password" />
		<property column="nombre" name="name" />
		<property column="apellido" name="surname" />
		<property column="locale" name="locale" />
	</class>
</hibernate-mapping>